<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml"
	schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
   <teiHeader>
      <fileDesc>
         <titleStmt>
            <title>titre</title>
         </titleStmt>
         <publicationStmt>
            <p>Publication Information</p>
         </publicationStmt>
         <sourceDesc>
            
            <msDesc xml:id="A">
               
               <!-- identifier le manuscrit -->
               
               <msIdentifier>
                  <country><!-- pays de conservation --></country>
                  <settlement><!-- ville de conservation --></settlement>
                  <repository><!--instituation chargée de la conservation du manuscrit--></repository>
                  <idno>lat.xxx</idno>
                  <!-- côte du manuscrit -->
                  <altIdentifier>
                     <!-- ancienne côte du manuscrit -->
                     <idno/>
                  </altIdentifier>
               </msIdentifier>
               
               <head>
                  <title><!-- titre du manuscrit --></title>
                  <origDate><!-- datation --></origDate>
                  <note><!-- informations complémentaires --></note>
               </head>
               
               <!-- structure du manuscrit -->
               <msContents>
                  <msItem>
                     <locus><!-- folios concernés par la section --></locus>
                     <author><!-- auteur --></author>
                     <title><!-- titre de la section --></title>
                     <incipit><!-- premiers mots de la section --></incipit>
                     <explicit><!-- derniers mots de la section --></explicit>
                  </msItem>
                  <msItem>
                     <locus><!-- folios concernés par la section --></locus>
                     <author><!-- auteur --></author>
                     <title><!-- titre de la section --></title>
                     <incipit><!-- premiers mots de la section --></incipit>
                     <explicit><!-- derniers mots de la section --></explicit>
                  </msItem>
                  <!-- A répéter autant de fois que nécessaire -->
                  
               </msContents>
               
               <!-- Description matérielle du manuscrit -->
               <physDesc>
                  <objectDesc>
                     <supportDesc>
                        <support><!-- matériaux --></support>
                        <extent>
                           <width unit="mm"/>
                           <height unit="mm"/>
                        </extent>
                        <foliation><!-- type de cahier, nombre de folios--></foliation>
                     </supportDesc>
                     <layoutDesc>
                        <!-- disposition du texte sur le folio -->
                        <layout columns="" writtenLines=""/>
                     </layoutDesc>
                  </objectDesc>
                  <handDesc hands=""> <!-- @hands = nombre de mains -->
                     <!-- Déclaration des mains -->
                     <handNote xml:id="">
                        <p><locus from="" to=""/><!-- passage concerné -->
                           <!-- Description des traits distinctifs de la main concernée --></p>
                     </handNote>
                  </handDesc>
                  <scriptDesc>
                     <scriptNote><!-- caractérisation de l'écriture --></scriptNote>
                  </scriptDesc>
                  <decoDesc>
                     <!-- description de la décoration -->
                     <decoNote>
                        <stamp> <!--  description d'un cachet --></stamp>
                     </decoNote> 
                  </decoDesc>
                  <bindingDesc><!-- description de la reliure --></bindingDesc>
                  <sealDesc><!-- description du ou des sceaux --></sealDesc>
               </physDesc>
               
               <!-- Histoire et transmission du manuscrit -->
               <history>
                  <origin><!-- lieu de production du manuscrit --></origin>
                  <provenance>
                     <p><!-- histoire de la transmission du manuscrit --></p>
                     <!-- indications bibliographique -->
                     <listBibl>
                        <bibl>
                           <author/>
                           <title/>
                           <pubPlace/>
                           <publisher/>
                           <date/>
                        </bibl>
                     </listBibl>
                  </provenance>
               </history>
            </msDesc>
            
         </sourceDesc>
      </fileDesc>
   </teiHeader>
   <text>
      <body>
         <p>Some text here.</p>
      </body>
   </text>
</TEI>
