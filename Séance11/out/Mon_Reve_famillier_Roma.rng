   <define name="tei_att.personal.attribute.full">
      <optional>
         <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                    name="full"
                    a:defaultValue="yes">
            <a:documentation>indique si la composante du nom est donnée en intégralité, sous forme d'abréviation ou simplement d'initiale.</a:documentation>
            <choice>
               <value>yes</value>
               <a:documentation>(complet) la composante du nom est orthographiée en intégralité.</a:documentation>
               <value>abb</value>
               <a:documentation>(abrégé) la composante du nom est donnée sous forme abrégée.</a:documentation>
